zone "wt18.ephec-ti.be" {
        type master;
        file "/etc/bind/db.wt18.ephec-ti.be";
        };
        
logging {
    channel security_file {
        file "/var/log/named/security.log" versions 3 size 30m;
        severity dynamic;
        print-time yes;
    };
    category security {
        security_file;
    };
};
